		<artifactId>maltcms-modules</artifactId>
		<groupId>net.sf.maltcms</groupId>
		<version>1.2.1-SNAPSHOT</version>
	</parent>
	<artifactId>maltcms-nbm-cluster</artifactId>
	<name>Maltcms Distribution NBM</name>
	<url>http://maltcms.sourceforge.net</url>
	<packaging>pom</packaging>
    <properties>
		<maven.nbm.skip>true</maven.nbm.skip>
	</properties>
	<modules>
		<module>db4o-nbm</module>
		<module>jai-nbm</module>
		<module>jfreechart-nbm</module>
    <module>maltcms-nbm</module>
  </modules>
	<build>
        <plugins>      
			<plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>nbm-maven-plugin</artifactId>
				<version>3.9</version>
                <extensions>true</extensions>
				<configuration>
					<useOSGiDependencies>false</useOSGiDependencies>
					<author>Nils Hoffmann</author>
					<cluster>maltcms</cluster>
					<publicPackages>
						<publicPackage>maltcms.*</publicPackage>
						<publicPackage>net.sf.maltcms.*</publicPackage>
						<!--<publicPackage>net.sf.mpaxs.*</publicPackage>-->
<!--						<publicPackage>org.apache.*</publicPackage>
						<publicPackage>groovy.*</publicPackage>
						<publicPackage>org.codehaus.groovy.*</publicPackage>
						<publicPackage>ucar.*</publicPackage>
						<publicPackage>thredds.*</publicPackage>
						<publicPackage>org.jfree.*</publicPackage>
						<publicPackage>javax.media.*</publicPackage>
						<publicPackage>javax.xml.bind.*</publicPackage>
						<publicPackage>com.sun.media.jai.*</publicPackage>
						<publicPackage>com.db4o.*</publicPackage>-->
                    </publicPackages>
					<brandingToken>maltcms-nbm</brandingToken>
					<defaultCluster>maltcms</defaultCluster>
					<codeNameBase>net.sf.maltcms.nbm</codeNameBase>
				</configuration>
            </plugin>
            <plugin> <!-- required since nbm-plugin 3.0-->
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-jar-plugin</artifactId>
                <configuration>
                    <useDefaultManifestFile>true</useDefaultManifestFile>
                </configuration>
            </plugin>
        </plugins>
    </build>
