<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <description>
            This file defines the default ChromA4D pipeline.
    </description>
    
    <import resource="chroma4D-base.xml"/>
<!--    <import resource="chroma4D-srg.xml"/>-->
<!--    <import resource="chroma4D-cwt.xml"/>-->
    
    <!-- a command pipeline consists of a list of 
     commands to be executed -->
    <bean id="commandPipeline" class="cross.datastructures.pipeline.CommandPipeline">
        <property name="checkCommandDependencies" value="true"/>
        <property name="commands">
            <list>
<!--                <ref bean="modulationTimeEstimator" />-->
                <ref bean="default2DVarLoader" />
                <ref bean="chromatogram2DVisualizer" />
<!--                <ref bean="seededRegionGrowing" />-->
            </list>
        </property>    
    </bean>

</beans>