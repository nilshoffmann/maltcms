<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <description>
        This file defines elements of the ChromA4D CWT peakfinder.
    </description>
    
    
               
    <bean id="cwtPeakFinder" class="maltcms.commands.fragments2d.peakfinding.cwt.CwtPeakFinder" scope="prototype">
        <property name="minScale" value="1"/>
        <property name="maxScale" value="50"/>
        <property name="maxNeighbors" value="10"/>
        <property name="radius" value="20.0"/>
    </bean>
   
</beans>
